{
  "name": "start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prepare": "husky",
    "postinstall": "node -e \"if (process.env.VERCEL !== '1' && process.env.CI !== 'true') { require('child_process').execSync('npm run db:types', {stdio: 'inherit'}); }\"",
    "clean": "rm -rf .next node_modules",
    "reinstall": "npm run clean && npm install",
    "dev:db": "concurrently \"npm run db:start\" \"npm run dev\"",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:migrate": "supabase migration new",
    "db:migrate:up": "supabase migration up",
    "db:migrate:list": "supabase migration list",
    "db:types": "supabase gen types typescript --local > src/types/database.generated.ts",
    "db:types:remote": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.generated.ts",
    "db:seed": "supabase db reset --db-url postgresql://postgres:postgres@localhost:54322/postgres && psql postgresql://postgres:postgres@localhost:54322/postgres -f supabase/seed/seed.sql",
    "db:seed:only": "psql postgresql://postgres:postgres@localhost:54322/postgres -f supabase/seed/seed.sql",
    "db:reset:hard": "psql postgresql://postgres:postgres@localhost:54322/postgres -f supabase/seed/reset.sql",
    "supabase:link": "supabase link",
    "supabase:functions:serve": "supabase functions serve",
    "supabase:functions:deploy": "supabase functions deploy"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.1",
    "@tanstack/react-query": "^5.81.5",
    "@uiw/react-md-editor": "^4.0.7",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.523.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.0",
    "supabase": "^2.26.9",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "web-vitals": "^5.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
